<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="50.0" prefWidth="880.0" style="-fx-background-color:  #ECECEC;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.mvmxpert.david.giczi.electricwireeditor.controller.ModifyTextController">
   <children>
      <TextField fx:id="inputTextField" layoutX="14.0" layoutY="10.0" prefHeight="31.0" prefWidth="332.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></TextField>
      <Button layoutX="358.0" layoutY="10.0" mnemonicParsing="false" onAction="#modifyInputText" prefHeight="31.0" prefWidth="51.0" text="Módosít">
         <font>
            <Font size="10.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="423.0" layoutY="10.0" mnemonicParsing="false" onAction="#moveInputTextLeft" prefHeight="31.0" prefWidth="51.0" text="←">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="487.0" layoutY="10.0" mnemonicParsing="false" onAction="#moveInputTextUp" prefHeight="31.0" prefWidth="51.0" text="↑">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="547.0" layoutY="10.0" mnemonicParsing="false" onAction="#moveInputTextDown" prefHeight="31.0" prefWidth="51.0" text="↓">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="609.0" layoutY="10.0" mnemonicParsing="false" onAction="#moveInputTextRight" prefHeight="31.0" prefWidth="51.0" text="→">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="670.0" layoutY="10.0" mnemonicParsing="false" onAction="#deleteInputText" prefHeight="31.0" prefWidth="51.0" text="Töröl">
         <font>
            <Font size="10.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="729.0" layoutY="10.0" mnemonicParsing="false" onAction="#rotateInputText" prefHeight="31.0" prefWidth="51.0" text="Forgat">
         <font>
            <Font size="10.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ComboBox fx:id="textSizeComboBox" layoutX="795.0" layoutY="10.0" onAction="#setSizeInputText" prefHeight="31.0" prefWidth="71.0" visibleRowCount="6">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
   </children>
</AnchorPane>
