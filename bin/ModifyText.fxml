<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="50.0" prefWidth="959.0" style="-fx-background-color:  #ECECEC;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.mvmxpert.david.giczi.electricwireeditor.controller.ModifyTextController">
   <children>
      <TextField fx:id="inputTextField" layoutX="14.0" layoutY="10.0" prefHeight="31.0" prefWidth="332.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></TextField>
         <Button layoutX="356.0" layoutY="10.0" mnemonicParsing="false" onAction="#deleteInputText" prefHeight="31.0" prefWidth="51.0" text="Töröl">
         <font>
            <Font size="10.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ColorPicker fx:id="colorPicker" layoutX="416.0" layoutY="10.0" prefHeight="31.0" prefWidth="97.0"/>
       <TextField fx:id="rotateValueTextField" layoutX="521.0" layoutY="10.0" prefHeight="31.0" prefWidth="51.0" />
       <ComboBox fx:id="textSizeComboBox" layoutX="578.0" layoutY="10.0" prefHeight="31.0" prefWidth="71.0" visibleRowCount="6">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <Button layoutX="659.0" layoutY="10.0" mnemonicParsing="false" onAction="#modifyInputText" prefHeight="31.0" prefWidth="51.0" text="Módosít">
         <font>
            <Font size="10.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
       <Button layoutX="776.0" layoutY="10.0" mnemonicParsing="false" onAction="#moveInputTextLeft" prefHeight="31.0" prefWidth="51.0" text="←">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="718.0" layoutY="10.0" mnemonicParsing="false" onAction="#moveInputTextUp" prefHeight="31.0" prefWidth="51.0" text="↑">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="834.0" layoutY="10.0" mnemonicParsing="false" onAction="#moveInputTextDown" prefHeight="31.0" prefWidth="51.0" text="↓">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="892.0" layoutY="10.0" mnemonicParsing="false" onAction="#moveInputTextRight" prefHeight="31.0" prefWidth="51.0" text="→">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
